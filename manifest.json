{
    "name": "wnacg download file name changer",
    "description": "",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Right",
                "mac": "MacCtrl+Right"
            }
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "downloads"
    ]
}